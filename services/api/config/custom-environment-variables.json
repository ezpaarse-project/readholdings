{
  "nodeEnv": "NODE_ENV",
  "timezone": "TIMEZONE",
  "apikey": "ADMIN_APIKEY",
  "smtp": {
    "host": "SMTP_HOST",
    "port": { "__name": "SMTP_PORT", "__format": "number" }
  },
  "notifications": {
    "sender": "NOTIFICATIONS_SENDER",
    "receivers": "NOTIFICATIONS_RECEIVERS",
    "machine": "NOTIFICATIONS_MACHINE"
  },
  "elasticsearch": {
    "nodes": "ELASTICSEARCH_NODES",
    "username": "ELASTICSEARCH_USERNAME",
    "password": "ELASTICSEARCH_PASSWORD",
    "timeout": { "__name": "ELASTICSEARCH_TIMEOUT", "__format": "number" }
  },
  "redis":  {
    "host": "REDIS_HOST",
    "port": { "__name": "REDIS_PORT", "__format": "number" },
    "password": "REDIS_PASSWORD"
  },
  "holdingsIQ": {
    "baseURL": "HOLDINGSIQ_BASEURL"
  },
  "portals": {
    "INC": {
      "apiKey": "INC_APIKEY",
      "custid": "INC_CUSTID"
    },
    "INSB": {
      "apiKey": "INSB_APIKEY",
      "custid": "INSB_CUSTID"
    },
    "INEE": {
      "apiKey": "INEE_APIKEY",
      "custid": "INEE_CUSTID"
    },
    "INSU": {
      "apiKey": "INSU_APIKEY",
      "custid": "INSU_CUSTID"
    },
    "INSHS": {
      "apiKey": "INSHS_APIKEY",
      "custid": "INSHS_CUSTID"
    },
    "INSIS": {
      "apiKey": "INSIS_APIKEY",
      "custid": "INSIS_CUSTID"
    },
    "INS2I": {
      "apiKey": "INS2I_APIKEY",
      "custid": "INS2I_CUSTID"
    },
    "INP": {
      "apiKey": "INP_APIKEY",
      "custid": "INP_CUSTID"
    },
    "IN2P3": {
      "apiKey": "IN2P3_APIKEY",
      "custid": "IN2P3_CUSTID"
    },
    "INSMI": {
      "apiKey": "INSMI_APIKEY",
      "custid": "INSMI_CUSTID"
    },
    "INTEST": {
      "apiKey": "INTEST_APIKEY",
      "custid": "INTEST_CUSTID"
    }
  },
  "cron": {
    "cleanFile": {
      "schedule": "CRON_CLEAN_FILE_SCHEDULE",
      "active": { "__name": "CRON_CLEAN_FILE_ACTIVE", "__format": "boolean" },
      "applicationLogThreshold": { "__name": "CRON_CLEAN_LOG_APPLICATION_THRESHOLD", "__format": "number" },
      "accessLogThreshold": { "__name": "CRON_CLEAN_LOG_ACCESS_THRESHOLD", "__format": "number" },
      "healthcheckLogThreshold": { "__name": "CRON_CLEAN_LOG_HEALTHCHECK_THRESHOLD", "__format": "number" },
      "holdingsIQThreshold": { "__name": "CRON_CLEAN_HOLDINGSIQ_THRESHOLD", "__format": "number" },
      "HLMThreshold": { "__name": "CRON_CLEAN_HLM_THRESHOLD", "__format": "number" }
    },
    "dataUpdate": {
      "schedule": "CRON_DATA_UPDATE_SCHEDULE",
      "active": { "__name": "CRON_DATA_UPDATE_ACTIVE", "__format": "boolean" }
    }
  }
}